    "@types/node": "^18.7.15",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "babel-plugin-macros": "^3.1.0",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
