    "@react-hook/size": "^2.1.2",
    "@react-hook/window-scroll": "^1.3.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@tanstack/react-query": "^4.13.0",
    "dompurify": "^2.4.0",
    "ethers": "^5.7.0",
    "file-saver": "^2.0.5",
