{
  "name": "ecn-next-web-frontend",
  "version": "0.1.0",
  "private": true,
  "author": "ECN",
  "scripts": {
    "postinstall": "patch-package && yarn prisma-generate",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "cd ../../ && husky install packages/ecn-web-frontend/.husky",
    "prisma-generate": "prisma generate --schema=./../ecn-prisma/prisma/schema.prisma"
  },
  "dependencies": {
