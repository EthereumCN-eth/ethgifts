{
  "name": "ecn-discord-bot",
  "version": "0.4.2",
  "description": "Boilerplate for TypeScript projects (Node.js and browser modules)",
  "author": "ecn",
  "repository": "https://github.com/metachris/typescript-boilerplate",
  "license": "MIT",
  "keywords": [
    "typescript",
    "boilerplate",
    "esbuild"
  ],
  "main": "./dist/tsc/main.js",
  "types": "./dist/tsc/main.d.ts",
  "scripts": {
    "dev": "ts-node-dev src/main.ts",
    "deploy-command": "ts-node src/deploy-commands.ts",
    "main": "ts-node src/main.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "esbuild-node": "esbuild src/main.ts --bundle --platform=node --minify --sourcemap=external --outfile=dist/esbuild/main.js",
    "esbuild-node:dev": "esbuild src/main.ts --bundle --platform=node --sourcemap=external --outfile=dist/esbuild/main.js",
    "esbuild-node:watch": "esbuild src/main.ts --bundle --platform=node --watch --sourcemap=external --outfile=dist/esbuild/main.js",
    "start": "yarn build && pm2 start dist/tsc/main.js"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^17.0.26",
    "@types/open-graph-scraper": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "jest": "27.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "@ethersproject/address": "^5.6.1",
    "axios": "^0.27.2",
    "discord-api-types": "^0.36.0",
    "discord-modals": "^1.3.9",
    "discord.js": "^13.9.1",
    "dotenv": "^16.0.1",
    "lodash.debounce": "^4.0.8",
    "open-graph-scraper": "^4.11.1",
    "pm2": "^5.2.0"
  }
}
